        MAX20342: MAX20342-i2c@35 {
                compatible = "maxiam,max20342";
                reg = <0x35>;

                pinctrl-names = "default";
		pinctrl-0 = <&max20342_int_pins>;	//	/* CD_IN# --gpio39*/ /* 0A5_FAULT# --gpio19 */

                host_power =<1>;
